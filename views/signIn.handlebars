<section id="signIn" class="leveled-form centered cf">
	<form id="signInForm">
		<label for="signInEmail">Log in to see existing albums:</label>
		<input type="email" placeholder="you@example.com" id="signInEmail" name="email">
		<input type="password" placeholder="password" id="signInPassword" name="password">
		<div class="inline-notification-area"></div>
		<input class="button primary" type="submit" id="signInInput" value="Sign in">
	</form>
</section>